  "scripts": {
    "init": "yarn install && npx husky install",
    "dev": "cross-env NODE_ENV=development BASE_ENV=development webpack-dev-server -c ./webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production BASE_ENV=production webpack -c ./webpack/webpack.prod.js",
    "build:speed": "cross-env NODE_ENV=production BASE_ENV=development webpack -c ./webpack/webpack.analy.js",
    "start:pre": "cross-env NODE_ENV=development BASE_ENV=preRelease webpack-dev-server -c ./webpack/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=development BASE_ENV=production webpack-dev-server -c ./webpack/webpack.dev.js",
    "build:dev": "cross-env NODE_ENV=production BASE_ENV=development webpack -c ./webpack/webpack.prod.js",
    "build:pre": "cross-env NODE_ENV=production BASE_ENV=preRelease webpack -c ./webpack/webpack.prod.js",
    "lint": "prettier -c --write \"src/**/*\"",
    "StyleLint": "prettier -c --write \"src/**/*\" && stylelint \"**/*.css\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git add . && cz && git push",
    "release": "release-it"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2"
  }
